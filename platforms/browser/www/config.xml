<?xml version='1.0' encoding='utf-8'?>
<widget id="com.toheed.crickettime" version="1.0.4" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>CricketTime</name>
    <description>
	Do you like cricket?
	Then CrickeTime is your perfect app to watch live cricket on.
	You can see a pointtable and fixtures of upcoming matches.
	Follow the match live now!
      
    </description>
    <author email="support@toheed.dk" href="www.toheed.dk">
        Toheed Sharif.
    </author>
    <content src="index.html" />
    <gap:plugin name="onesignal-cordova-plugin" source="npm" spec="^2.4.1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="15" />
    <preference name="android-targetSdkVersion" value="28" />
    <preference name="android-build-tool" value="gradle" />
    <preference name="phonegap-version" value="cli-8.0.0" />
</widget>
